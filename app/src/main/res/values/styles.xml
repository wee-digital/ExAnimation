<resources>

    <!-- TEXT VIEW. -->
    <style name="TextView" parent="WrapContent">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:text">TextView</item>
    </style>

    <style name="TextView2" parent="TextView">
        <item name="android:textSize">@dimen/textSize2</item>
    </style>

    <style name="TextView3" parent="TextView">
        <item name="android:textSize">@dimen/textSize3</item>
    </style>

    <style name="TextView4" parent="TextView">
        <item name="android:textSize">@dimen/textSize4</item>
    </style>

    <style name="TextView5" parent="TextView">
        <item name="android:textSize">@dimen/textSize5</item>
    </style>

    <style name="Headline1SemiBold" parent="TextView">
        <item name="android:textSize">@dimen/_19ssp</item>
        <item name="fontFamily">@font/inter_semibold</item>
        <item name="android:text">Headline1SemiBold</item>
    </style>

    <style name="Headline1Regular" parent="TextView">
        <item name="android:textSize">@dimen/_19ssp</item>
        <item name="android:text">Headline1Regular</item>
    </style>

    <style name="Headline2" parent="TextView">
        <item name="android:textSize">@dimen/_15ssp</item>
        <item name="android:gravity">center</item>
        <item name="android:text">Headline2</item>
    </style>

    <style name="Headline3" parent="TextView">
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:text">Headline3</item>
    </style>

    <style name="BodySemibold" parent="TextView">
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:text">BodySemibold</item>
    </style>

    <style name="BodyRegular" parent="TextView">
        <item name="android:textSize">@dimen/_9ssp</item>
        <item name="android:text">BodyRegular</item>
    </style>

    <style name="DescriptionSemibold" parent="TextView">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textSize">@dimen/_8ssp</item>
        <item name="android:text">DescriptionSemibold</item>
    </style>

    <style name="DescriptionRegular" parent="TextView">
        <item name="android:textSize">@dimen/_8ssp</item>
        <item name="android:text">DescriptionRegular</item>
    </style>

    <!-- EDIT TEXT -->
    <style name="EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_32sdp</item>
        <item name="android:paddingStart">@dimen/margin</item>
        <item name="android:paddingEnd">@dimen/margin</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:maxLines">1</item>
        <item name="android:inputType">textEmailAddress</item>
        <item name="android:textColor">@color/color_black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColorHint">@color/color_gray_2</item>
        <item name="android:drawablePadding">14dp</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:includeFontPadding">true</item>
        <item name="android:textCursorDrawable">@color/colorPrimary</item>
        <item name="android:textIsSelectable">false</item>
        <item name="android:privateImeOptions">nm</item>
        <item name="android:longClickable">false</item>
    </style>

    <!--BUTTON-->
    <style name="Button" parent="TextView2">
        <item name="android:minWidth">@dimen/_100sdp</item>
        <item name="android:layout_height">@dimen/_30sdp</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:text">Button</item>
        <item name="android:textSize">@dimen/_10ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">#747474</item>
    </style>

    <style name="Button.Primary">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/drw_btn_primary</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="Button.Semibold">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/drw_btn_primary</item>
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:elevation">1px</item>
    </style>

    <!--DIALOG-->
    <style name="Dialog" />

    <style name="Dialog.BottomFragment">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:minHeight">@dimen/_200sdp</item>
        <item name="android:background">@drawable/drw_dialog_bottom</item>
        <item name="android:paddingTop">@dimen/marginShadow</item>
    </style>

    <style name="Dialog.Title" parent="TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="android:background">@drawable/drw_dialog_bottom</item>
        <item name="android:backgroundTint">#fff</item>
        <item name="android:textColor">@color/color_black</item>
        <item name="android:textSize">@dimen/_9ssp</item>
        <item name="android:gravity">center</item>
        <item name="android:text">Dialog title</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:paddingBottom">@dimen/_2sdp</item>
    </style>

    <style name="Dialog.Text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/drw_dialog_bottom</item>
        <item name="android:backgroundTint">#fff</item>
        <item name="android:textColor">#A0A0A0</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:gravity">center</item>
        <item name="android:text">Cửa hàng</item>
    </style>

    <style name="Dialog.TitleIcon">
        <item name="android:layout_width">@dimen/_40sdp</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="android:tint">#A0A0A0</item>
        <item name="android:padding">@dimen/_14sdp</item>
    </style>

    <!--MESSAGE-->
    <style name="Message" />

    <style name="Message.Icon">
        <item name="android:layout_width">@dimen/_54sdp</item>
        <item name="android:layout_height">@dimen/_54sdp</item>
        <item name="android:layout_marginBottom">@dimen/margin2</item>
        <item name="android:layout_marginTop">@dimen/margin2</item>
    </style>

    <style name="Message.Title" parent="TextView4">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_marginTop">@dimen/margin2</item>
        <item name="android:layout_marginBottom">@dimen/margin2</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
    </style>

    <style name="Message.Text" parent="TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin2</item>
        <item name="android:layout_marginBottom">@dimen/margin5</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">@dimen/_11ssp</item>
    </style>

    <!--SPLASH-->
    <style name="Splash" />

    <!--PAYMENT-->
    <style name="Payment" />

    <style name="Payment.Title" parent="TextView2">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
    </style>

    <style name="Payment.Property" parent="TextView2">
        <item name="android:textColor">@color/colorGray</item>
        <item name="android:gravity">center</item>
    </style>

    <!--CONNECT-->
    <style name="Connect" />

    <style name="Connect.HelpIcon" parent="TextView">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">@dimen/_11sdp</item>
        <item name="android:layout_height">@dimen/_11sdp</item>
        <item name="android:background">@drawable/bg_oval_outline</item>
        <item name="android:backgroundTint">@color/colorPrimary</item>
        <item name="android:textColor">@color/colorBlue</item>
        <item name="android:textSize">@dimen/_6ssp</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:layout_marginTop">@dimen/margin2</item>
    </style>

    <style name="Connect.HelpText" parent="TextView2">
        <item name="android:layout_width">0dp</item>
        <item name="android:minWidth">100dp</item>
        <item name="android:layout_marginStart">@dimen/margin</item>
        <item name="android:layout_marginEnd">@dimen/margin2</item>
        <item name="android:includeFontPadding">true</item>
    </style>

    <!--GRADIENT TEXT-->
    <style name="GradientText" parent="TextView5">
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
    </style>

    <!--TEST-->
    <style name="TestingWidget">
        <item name="android:layout_width">@dimen/_40sdp</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="android:background">@color/colorTransparent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>

    <!--GUIDELINE-->
    <style name="Guideline">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Guideline.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="Guideline.Header" parent="Guideline.Horizontal">
        <item name="layout_constraintGuide_percent">0.078</item>
    </style>

    <style name="Guideline.Face" parent="Guideline.Horizontal">
        <item name="layout_constraintGuide_percent">0.40</item>
    </style>

    <style name="Guideline.Splash" parent="Guideline.Horizontal">
        <item name="layout_constraintGuide_percent">0.20</item>
    </style>

    <style name="Widget.App.Theme.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="Widget.App.Theme.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

    <!--GUIDELINE-->
    <style name="Remaining" />

    <style name="Remaining.Text" parent="TextView">
        <item name="android:layout_marginTop">@dimen/margin</item>
        <item name="android:gravity">center</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="ImageView" />

    <style name="ImageView.Progress">
        <item name="android:layout_width">@dimen/_38sdp</item>
        <item name="android:layout_height">@dimen/_38sdp</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:clickable">false</item>
        <item name="android:visibility">invisible</item>
    </style>
</resources>
